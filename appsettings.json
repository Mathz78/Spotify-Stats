{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "SpotifyConnectionString": "server=<yourServerName>,<yourDataBasePort>;database=<yourDataBaseName>;uid=<yourDataBaseUserID>;password=<yourDataBasePassword>"
    },
    "ApiSettings": {
        "SpotifySettings": {
            "ClientId": "<clientId>",
            "ClientSecret": "<clientSecret>",
            "RedirectUri": "<redirectUri>",
            "BaseSpotifyUrl": "<baseSpotifyUrl>",
            "SpotifyScope": "<scopePriviligies>",
            "SpotifyUrls": {
                "spotifyApi": "https://api.spotify.com",
                "spotifyAccounts": "https://accounts.spotify.com"
            }
        },
        "JwtSecretToken": "<yourSecretJasonWebToken>",
        "AllowedOrigins": [
            "<firstSecretURL>", 
            "<secondSecretURL>"
        ]
    }
}